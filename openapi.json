  "info": {
    "title": "Habit Tracker API",
    "description": "API for habit tracking, community, challenges and gamification",
    "version": "1.0",
    "contact": {}
  },
  "tags": [
    {
      "name": "auth",
      "description": "Authentication (Better Auth)"
    },
    {
      "name": "profile",
      "description": "Profile, settings, follow, suggestions"
    },
    {
      "name": "tasks",
      "description": "Tasks and completions"
    },
    {
      "name": "posts",
      "description": "Posts, feed, comments, likes"
    },
    {
      "name": "groups",
      "description": "Groups and members"
    },
    {
      "name": "challenges",
      "description": "Challenges and progress"
    },
    {
      "name": "notifications",
      "description": "User notifications"
    },
    {
      "name": "gamification",
      "description": "Rewards and achievements"
    },
    {
      "name": "upload",
      "description": "Image upload (Cloudinary)"
    }
  ],
  "servers": [],
  "components": {
    "securitySchemes": {
      "bearer": {
        "scheme": "bearer",
        "bearerFormat": "JWT",
        "type": "http"
      }
    },
    "schemas": {
      "LogCompletionDto": {
        "type": "object",
        "properties": {
          "value": {
            "type": "number",
            "description": "Numeric value (e.g. for habits)"
          },
          "status": {
            "type": "string"
          },
          "notes": {
            "type": "string"
          },
          "description": {
            "type": "string"
          }
        }
      },
      "UpdateTaskDto": {
        "type": "object",
        "properties": {
          "title": {
            "type": "string"
          },
          "categoryId": {
            "type": "object"
          },
          "iconName": {
            "type": "object"
          },
          "colorValue": {
            "type": "object"
          },
          "imageUrl": {
            "type": "object"
          },
          "isPublic": {
            "type": "boolean"
          },
          "startDate": {
            "format": "date-time",
            "type": "string"
          },
          "endDate": {
            "type": "object"
          },
          "notes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "goalValue": {
            "type": "number"
          },
          "currentValue": {
            "type": "number"
          },
          "unitId": {
            "type": "object"
          },
          "direction": {
            "type": "string"
          },
          "steps": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "startTime": {
            "type": "object"
          },
          "dueTime": {
            "type": "object"
          },
          "priority": {
            "type": "string"
          },
          "isFlagged": {
            "type": "boolean"
          },
          "url": {
            "type": "object"
          },
          "affirmation": {
            "type": "object"
          },
          "durationMinutes": {
            "type": "object"
          },
          "reminders": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "subtasks": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "frequency": {
            "type": "object"
          },
          "pomodoroSettings": {
            "type": "object"
          }
        }
      },
      "CreateCategoryDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Health"
          },
          "iconName": {
            "type": "object"
          },
          "colorValue": {
            "type": "object"
          },
          "imageUrl": {
            "type": "object"
          }
        },
        "required": [
          "name"
        ]
      },
      "CreateUnitDto": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "example": "Kilometers"
          },
          "symbol": {
            "type": "string",
            "example": "km"
          }
        },
        "required": [
          "name",
          "symbol"
        ]
      },
      "UpdateProfileDto": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
          },
          "bio": {
            "type": "string",
            "maxLength": 500
          },
          "isTaggingAllowed": {
            "type": "boolean"
          },
          "avatarUrl": {
            "type": "object"
          }
        }
      },
      "UpdateProfileSettingsDto": {
        "type": "object",
        "properties": {
          "isSearchable": {
            "type": "boolean"
          },
          "analyticsEnabled": {
            "type": "boolean"
          },
          "profileVisibility": {
            "type": "string",
            "enum": [
              "PUBLIC",
              "PRIVATE"
            ]
          },
          "challengeVisibility": {
            "type": "string",
            "enum": [
              "PUBLIC",
              "PRIVATE"
            ]
          },
          "challengePostVisibility": {
            "type": "string",
            "enum": [
              "PUBLIC",
              "PRIVATE"
            ]
          },
          "taskDailyReminderTime": {
            "type": "object"
          },
          "taskWeekStartDay": {
            "type": "number",
            "minimum": 0,
            "maximum": 6
          },
          "taskArchiveVisible": {
            "type": "boolean"
          },
          "pomodoroFocusDuration": {
            "type": "number",
            "minimum": 1
          },
          "pomodoroBreakDuration": {
            "type": "number",
            "minimum": 1
          },
          "pomodoroLongBreakDuration": {
            "type": "number",
            "minimum": 1
          }
        }
      },
      "AddCommentDto": {
        "type": "object",
        "properties": {
          "content": {
            "type": "string",
            "maxLength": 5000,
            "example": "Great progress!"
          }
        },
        "required": [
          "content"
        ]
      },
      "UploadImageBodyDto": {
        "type": "object",
        "properties": {
          "folder": {
            "type": "string",
            "description": "Cloudinary folder path"
          },
          "publicIdPrefix": {
            "type": "string",
            "description": "Prefix for public_id"
          }
        }
      }
    }
  }
}